<p id="notice"><%= notice %></p>

<h1>Business Models</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @business_models.each do |business_model| %>
      <tr>
        <td><%= business_model.name %></td>
        <td><%= link_to 'Show', business_model %></td>
        <td><%= link_to 'Edit', edit_business_model_path(business_model) %></td>
        <td><%= link_to 'Destroy', business_model, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Business Model', new_business_model_path %>
